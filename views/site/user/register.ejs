<% include ../partials/header %>
<div class="container">
				<div class="row">
					<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
						<!-- BSTORE-BREADCRUMB START -->
						<div class="bstore-breadcrumb">
							<a href="/">Trang chủ</a>
							<span><i class="fa fa-caret-right"></i></span>
							<span>Đăng ký</span>
						</div>
						<!-- BSTORE-BREADCRUMB END -->
					</div>
				</div>
				<div class="row">
					<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
						<h2 class="page-title">Đăng ký</h2>
					</div>
					<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
						<!-- CREATE-NEW-ACCOUNT START -->
						<img src="/images/frontend/shop-thoi-trang-min.jpg" class="img-fluid ${3|rounded-top,rounded-right,rounded-bottom,rounded-left,rounded-circle,|}" alt="">
						<!-- CREATE-NEW-ACCOUNT END -->
					</div>
					<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
						<!-- REGISTERED-ACCOUNT START -->
						<div class="primari-box registered-account">
							<form class="new-account-box" id="accountLogin" method="post" action="#">
								<h3 class="box-subheading">Thông tin người dùng</h3>
								<div class="form-content">
									<div class="form-group primary-form-group">
										<label for="loginemail">Địa chỉ email</label>
										<input type="text" value="" name="email" id="loginemail" class="form-control input-feild">
									</div>
									<div class="form-group primary-form-group">
										<label for="password">Mật khẩu</label>
										<input type="password" value="" name="password" id="password" class="form-control input-feild">
									</div>
                                    <div class="form-group primary-form-group">
										<label for="password">Nhập lại mật khẩu</label>
										<input type="password" value="" name="password-confirm" id="password" class="form-control input-feild">
									</div>
                                    <div class="form-group primary-form-group">
										<label for="fullname">Họ tên</label>
										<input type="text" value="" name="fullname" id="fullname" class="form-control input-feild">
									</div>
									<div class="form-group primary-form-group">
										<label for="image">Ảnh</label>
										<input type="file" value="" name="image" id="image" class="form-control input-feild">
									</div>
									<div class="form-check">
                                        <label class="">Giới tính
                                        </label>
                                        </br>
                                        <input type="radio" class="form-check-input" name="" id="" value="checkedValue" checked>
                                        Nam
                                        <input type="radio" class="form-check-input" name="" id="" value="checkedValue" checked>
                                        Nữ
                                    </div>
									<div class="form-group primary-form-group">
										<label for="birthday">Ngày sinh</label>
										<input type="date" value="" name="birthday" id="birthday" class="form-control input-feild">
									</div>
									<div class="form-group primary-form-group">
										<label for="address">Địa chỉ</label>
										<input type="text" value="" name="address" id="address" class="form-control input-feild">
									</div>
									<div class="submit-button">
										<a href="my-account.html" id="signinCreate" class="btn main-btn">
										<span>
                                            <i class="fa fa-user-plus" aria-hidden="true"></i>
											Đăng ký
                                            <i class="fa fa-chevron-right" aria-hidden="true"></i>
										</span>
										</a>
									</div>
								</div>
							</form>							
						</div>
						<!-- REGISTERED-ACCOUNT END -->
					</div>
				</div>
			</div>
			<% include ../partials/footer %>